{
    "name": "pricecurrent/laravel-eloquent-filters",
    "description": "Advanced Laravel models filtering capabilities",
    "keywords": [
        "pricecurrent",
        "laravel",
        "laravel-eloquent-filters",
        "eloquent-filters",
        "model-filters"
    ],
    "homepage": "https://github.com/pricecurrent/laravel-eloquent-filters",
    "license": "MIT",
    "authors": [{
        "name": "Andrew Malinnikov",
        "email": "code.bright.anywhere@gmail.com",
        "role": "Developer"
    }],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0|^9.0",
        "illuminate/support": "^8.0|^9.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^6.23|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pricecurrent\\LaravelEloquentFilters\\": "src",
            "Pricecurrent\\LaravelEloquentFilters\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pricecurrent\\LaravelEloquentFilters\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pricecurrent\\LaravelEloquentFilters\\EloquentFiltersServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
